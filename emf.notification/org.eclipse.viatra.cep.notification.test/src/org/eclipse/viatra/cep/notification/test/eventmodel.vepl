package org.eclipse.viatra.cep.notification.test

import org.eclipse.viatra.cep.emf.notification.model.*

complexEvent setTwice(feature : Object){
	as SET(_, feature, _, _){2}
}

rule r on setTwice{
	System.out.println(String.format("The same feature has been set twice (%s)", ruleInstance.atom.observedEventPatternId))
}