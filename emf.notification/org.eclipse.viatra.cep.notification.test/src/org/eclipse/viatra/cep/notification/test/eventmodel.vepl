package org.eclipse.viatra.cep.notification.test

import org.eclipse.viatra.cep.emf.notification.model.*

ComplexEvent setTwice(feature : Object){
	definition: SET(feature){2}
}

Rule r{
	events: setTwice
	action{
		System.out.println(String.format("The same feature has been set twice (%s)", activation.atom.observableEventPattern.id))
	}
}